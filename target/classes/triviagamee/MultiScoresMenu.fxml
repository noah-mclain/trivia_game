<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="872.0" prefWidth="1109.0" stylesheets="@Font.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="triviagamee.MultiScoresMenuController">
    <children>
        <ImageView fitHeight="872.0" fitWidth="1113.0" layoutX="-3.0" pickOnBounds="true">
            <image>
                <Image url="@../photos/LeaderBoard.jpeg" />
            </image>
        </ImageView>
        <Button layoutX="286.0" layoutY="768.0" mnemonicParsing="false" onAction="#switchToGenresAgain" styleClass="answers" text="Next Round!" textFill="#57dfe1">
            <font>
                <Font size="36.0" />
            </font>
            <effect>
                <Glow level="1.0" />
            </effect></Button>
        <Button layoutX="639.0" layoutY="758.0" mnemonicParsing="false" onAction="#switchToHomepageAgain" prefHeight="68.0" prefWidth="180.0" styleClass="answers" text="Bayiee!" textFill="#d153ff">
            <font>
                <Font size="36.0" />
            </font>
            <effect>
                <Glow level="1.0" />
            </effect></Button>
      <VBox layoutX="162.0" layoutY="195.0" prefHeight="524.0" prefWidth="776.0" styleClass="leaderBoard">
         <children>
            <TableView prefHeight="534.0" prefWidth="776.0">
              <columns>
                <TableColumn prefWidth="376.0" text="Players" />
                <TableColumn prefWidth="393.0" text="Scores" />
              </columns>
               <styleClass>
                  <String fx:value="leaderBoard" />
                  <String fx:value="column-header" />
               </styleClass>
               <effect>
                  <Glow level="1.0" />
               </effect>
            </TableView>
         </children>
      </VBox>
    </children>
</AnchorPane>
