<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="triviagamee.PlayerWaitingRoom"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label fx:id="waitingLabel" layoutX="142.0" layoutY="100.0" text="Waiting for host to start game..." AnchorPane.topAnchor="100.0" AnchorPane.leftAnchor="142.0" AnchorPane.rightAnchor="142.0" />
        <TextArea fx:id="userUpdate" layoutX="50.0" layoutY="150.0" prefHeight="200.0" prefWidth="500.0" wrapText="true"
                  AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
        <Label fx:id="editableLabel" layoutX="50.0" layoutY="360.0" text="You are able to chat" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="50.0" />
    </children>

</AnchorPane>
